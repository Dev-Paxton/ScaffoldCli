{
  "name": "scaffoldcli",
  "version": "1.0.0",
  "description": "Cli to scaffold projects",
  "main": "build/index.js",
  "scripts": {
    "build": "tsc && cp -r assets build && find build -type f -name '.gitkeep' -exec rm -f {} + && npm link"
  },
  "bin": {
    "scaffold": "build/index.js"
  },
