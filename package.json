    "url": "https://github.com/Dev-Paxton/ScaffoldCli/issues"
  },
  "homepage": "https://github.com/Dev-Paxton/ScaffoldCli#readme",
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "eslint": "^9.8.0",
    "globals": "^15.9.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.0.1"
  },
  "dependencies": {
    "@inquirer/prompts": "^5.3.8"
  }
}
